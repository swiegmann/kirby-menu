{
    "name": "swiegmann/kirby-menu",
    "description": "This Kirby CMS-Plugin lets you build and render structured menues with a variety of features.",
    "license": "MIT",
    "type": "kirby-plugin",
    "version": "1.0.3",
    "homepage": "https://github.com/swiegmann/kirby-menu",
    "keywords": [
        "kirby-plugin",
        "navigation",
        "menu"
    ],
    "authors": [{
        "name": "S.Wiegmann"
    }],
    "require": {
        "php": ">=8.1.0 <8.4.0",
        "getkirby/cms": "^4.0",
        "getkirby/composer-installer": "^1.2"
    },
    "extra": {
        "installer-name": "kirby-menu",
        "kirby-cms-path": false
	  },
    "config": {
        "allow-plugins": {
            "getkirby/composer-installer": true
        }
    }
}